   <string notr="true">/* 整个登录窗口背景 */
QWidget {
    background-color: #f5f5f5;
}

/* 标题标签（对应titleLable）- 参考图二的绿色标题栏 */
titleLable {
    background-color: #2ecc71; /* 图二的绿色主色调 */
    color: white;
    font-size: 20px;
    font-weight: bold;
    padding: 10px;
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
}

/* 关闭按钮（btnExit）- 标题栏右侧的按钮 */
btnExit {
    background-color: transparent;
    color: white;
    font-size: 16px;
    border: none;
    padding: 5px 10px;
}
btnExit:hover {
    background-color: #e74c3c;
}

/* 账号/密码标签（labelUser、labelPass） */
labelUser, labelPass {
    color: #333;
    font-size: 14px;
    padding-right: 10px;
}

/* 账号输入框（editUser）、密码输入框（editPass） */
editUser, editPass {
    border: 1px solid #ddd;
    border-radius: 3px;
    padding: 6px;
    font-size: 14px;
    background-color: white;
}
editUser:focus, editPass:focus {
    border-color: #2ecc71;
    outline: none;
}

/* 登录按钮（btnLogin）- 参考图二的绿色按钮 */
btnLogin {
    background-color: #2ecc71;
    color: white;
    font-size: 16px;
    font-weight: bold;
    border: none;
    border-radius: 5px;
    padding: 8px 20px;
}
btnLogin:hover {
    background-color: #27ae60;
}
btnLogin:disabled {
    background-color: #bdc3c7;
}

/* 注册按钮（loginUp）- 辅助按钮 */
loginUp {
    background-color: transparent;
    color: #2ecc71;
    font-size: 14px;
    border: none;
    padding: 5px 10px;
}
loginUp:hover {
    color: #27ae60;
    text-decoration: underline;
}</string>
